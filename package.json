{
  "name": "skateboard",
  "private": false,
  "version": "1.0.3",
  "type": "module",
  "workspaces": [
    "backend"
  ],
  "overrides": {
    "cookie": "0.7.2"
  },
  "scripts": {
    "start": "npm run front & npm run server",
    "front": "vite --force --mode development",
    "server": "npm run --workspace=backend start",
    "build": "vite build --mode development;",
    "prod": "vite build --mode production;",
    "install-all": "npm install && npm install --workspace=backend"
  },
  "dependencies": {
    "@stevederico/skateboard-ui": "1.0.9",
    "cookie": "0.7.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "tailwindcss": "4.1.14",
    "vite": "^7.1.10"
  }
}
