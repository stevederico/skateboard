{
  "name": "skateboard",
  "private": false,
  "version": "0.7.4",
  "type": "module",  
  "workspaces": [
    "backend"
  ],
  "scripts": {
    "start": "npm run front & npm run server",
    "front": "vite --force --mode development",
    "server": "npm run --workspace=backend start",
    
    "build": "vite build --mode development;",
    "prod": "vite build --mode production;",
    "install-all": "npm install && npm install --workspace=backend"
  },
  "dependencies": {
    "@stevederico/skateboard-ui": "^0.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "tailwindcss": "4.1.14",
    "vite": "^7.1.9"
  }
}
