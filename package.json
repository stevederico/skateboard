{
  "name": "skateboard",
  "private": true,
  "version": "0.1.4",
  "type": "module",
  "workspaces": [
    "backend"
  ],
  "scripts": {
    "serve": "vite --force --mode development",
    "start": "vite --force --mode development",
    "build": "vite build --mode development;",
    "prod": "vite build --mode production;",
    "server": "npm run start --workspace=backend",
    "dev": "npm run start & npm run server",
    "install-all": "npm install && npm install --workspace=backend"
  },
  "dependencies": {
    "@stevederico/skateboard-ui": "^0.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.7",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "tailwindcss": "4.0.0",
    "vite": "^6.3.5"
  }
}
