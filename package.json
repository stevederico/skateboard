{
  "name": "skateboard",
  "private": false,
  "version": "1.6.0",
  "skateboardVersion": "1.6.0",
  "type": "module",
  "workspaces": [
    "backend"
  ],
  "scripts": {
    "start": "npm run front & npm run server",
    "front": "vite --force --mode development",
    "server": "npm run --workspace=backend start",
    "build": "vite build --mode production",
    "prod": "vite build --mode production",
    "install-all": "npm install && npm install --workspace=backend"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@stevederico/skateboard-ui": "^1.5.2",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-table": "^8.21.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "tailwindcss": "^4.1.14",
    "vite": "^7.1.10"
  }
}
